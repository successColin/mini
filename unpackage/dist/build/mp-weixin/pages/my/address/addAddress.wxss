.uni-list-cell{display:flex;margin-left:40rpx}.tags{width:100rpx;height:40rpx;line-height:40rpx;text-align:center;background-color:#f8f8f8;border-radius:20rpx;font-size:24rpx}.active-tags{background-color:#fee;color:#d91b1b}.bottom-btn{display:flex;justify-content:center;position:fixed;bottom:200rpx;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.bottom-btn .btn-save{font-size:28rpx;width:300rpx;height:80rpx;line-height:80rpx;background:#d91b1b;border-radius:40rpx;color:#fff;text-align:center;font-weight:700;display:inline-block}.bottom-btn .btn-delete{font-size:28rpx;width:300rpx;height:80rpx;line-height:80rpx;background:#999;border-radius:40rpx;color:#fff;text-align:center;font-weight:700;display:inline-block}.default{margin-top:20rpx;background:#fff;padding:38rpx 51rpx 38rpx 51rpx;display:flex;justify-content:space-between}